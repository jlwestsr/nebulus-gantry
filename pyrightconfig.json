{
  "include": [
    "backend"
  ],
  "exclude": [
    "**/__pycache__",
    "**/.pytest_cache",
    "**/node_modules",
    "**/.venv",
    "**/venv",
    "frontend"
  ],
  "typeCheckingMode": "basic",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.12",
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": "backend",
      "pythonVersion": "3.12",
      "extraPaths": [
        "backend"
      ]
    }
  ],
  "venvPath": ".",
  "venv": "venv"
}
